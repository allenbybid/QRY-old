<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bid.allenby.qry" >
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.permission.ACCESS_CHECKIN_PROPERTIES"/>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
	      	android:resizeableActivity = "true"
       android:supportsRtl="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
		<activity
            android:name=".show"
            android:configChanges="orientation|keyboardHidden|screenLayout|screenSize"></activity>
		<activity
            android:name=".meanc"></activity>
		<activity
            android:name=".about"></activity>
		<activity
            android:name=".history"></activity>
		<activity
            android:name=".caches"></activity>
   <activity
            android:name=".caches2"></activity>
		<activity
            android:name=".opencaches"></activity>
		<activity
            android:name=".settings"></activity>
		<activity
            android:name=".InfoGet">
			<intent-filter>
				<action android:name="bid.login.abqry.MYACTION"/> 
				<action android:name="android.intent.action.VIEW"/>
				<category android:name="android.intent.category.DEFAULT"/>
				<data android:scheme="abqry"/>
			</intent-filter>
		</activity>
    </application>

</manifest>
